<p id="notice"><%= notice %></p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Publisher</th>
      <th>Publication Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resources.each do |resource| %>
      <tr>
        <td><%= title(resource) %></td>
        <td><%= publisher(resource) %></td>
        <td><%= publication_year(resource) %></td>
        <td><%= link_to 'Edit', generals_edit_path(resource_id: resource.id), class: "btn btn-info" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New Dataset", main_app.new_resource_path %>

